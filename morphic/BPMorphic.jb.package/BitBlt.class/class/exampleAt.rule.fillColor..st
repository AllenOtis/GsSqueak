private
exampleAt: originPoint rule: rule fillColor: mask 	"This builds a source and destination form and copies the source to the	destination using the specifed rule and mask. It is called from the	method named exampleOne. Only works with Display depth of 1"	| _gsSqueakTemp_s _gsSqueakTemp_d _gsSqueakTemp_border _gsSqueakTemp_aBitBlt |	_gsSqueakTemp_border := Form extent: 32 @ 32.	_gsSqueakTemp_border fillBlack.	_gsSqueakTemp_border		fill: (1 @ 1 extent: 30 @ 30)		fillColor: Color white.	_gsSqueakTemp_s := Form extent: 32 @ 32.	_gsSqueakTemp_s fillWhite.	_gsSqueakTemp_s		fillBlack: (7 @ 7 corner: 25 @ 25).	_gsSqueakTemp_d := Form extent: 32 @ 32.	_gsSqueakTemp_d fillWhite.	_gsSqueakTemp_d		fillBlack: (0 @ 0 corner: 32 @ 16).	_gsSqueakTemp_s displayOn: Display at: originPoint.	_gsSqueakTemp_border		displayOn: Display		at: originPoint		rule: Form under.	_gsSqueakTemp_d displayOn: Display at: originPoint + (_gsSqueakTemp_s width @ 0).	_gsSqueakTemp_border		displayOn: Display		at: originPoint + (_gsSqueakTemp_s width @ 0)		rule: Form under.	_gsSqueakTemp_d displayOn: Display at: originPoint + (_gsSqueakTemp_s extent // (2 @ 1)).	_gsSqueakTemp_aBitBlt := BitBlt				destForm: Display				sourceForm: _gsSqueakTemp_s				fillColor: mask				combinationRule: rule				destOrigin: originPoint + (_gsSqueakTemp_s extent // (2 @ 1))				sourceOrigin: 0 @ 0				extent: _gsSqueakTemp_s extent				clipRect: Display computeBoundingBox.	_gsSqueakTemp_aBitBlt copyBits.	_gsSqueakTemp_border		displayOn: Display		at: originPoint + (_gsSqueakTemp_s extent // (2 @ 1))		rule: Form under