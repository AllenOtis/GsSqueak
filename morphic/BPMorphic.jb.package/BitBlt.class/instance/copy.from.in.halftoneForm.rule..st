copying
copy: destRectangle from: sourcePt in: srcForm halftoneForm: hf rule: rule 	| _gsSqueakTemp_destOrigin |	sourceForm := srcForm.	self fillColor: hf.	"sets halftoneForm"	combinationRule := rule.	_gsSqueakTemp_destOrigin := destRectangle origin.	destX := _gsSqueakTemp_destOrigin x.	destY := _gsSqueakTemp_destOrigin y.	sourceX := sourcePt x.	sourceY := sourcePt y.	width := destRectangle width.	height := destRectangle height.	self copyBits