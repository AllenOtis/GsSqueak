private
colorConvertingMap: targetColor from: sourceDepth to: destDepth keepSubPixelAA: keepSubPix 	| _gsSqueakTemp_srcIndex _gsSqueakTemp_map _gsSqueakTemp_mapsForSource _gsSqueakTemp_mapsForSourceAndDest |	ColorConvertingMaps class == Array		ifFalse: [ColorConvertingMaps := (1 to: 10)						collect: [:i | Array new: 32]].	_gsSqueakTemp_srcIndex := sourceDepth.	sourceDepth > 8		ifTrue: [_gsSqueakTemp_srcIndex := keepSubPix						ifTrue: [9]						ifFalse: [10]].	_gsSqueakTemp_mapsForSource := ColorConvertingMaps at: _gsSqueakTemp_srcIndex.	_gsSqueakTemp_mapsForSourceAndDest := (_gsSqueakTemp_mapsForSource at: destDepth)				ifNil: [_gsSqueakTemp_mapsForSource at: destDepth put: Dictionary new].	_gsSqueakTemp_map := _gsSqueakTemp_mapsForSourceAndDest				at: targetColor				ifAbsentPut: [Color						computeColorConvertingMap: targetColor						from: sourceDepth						to: destDepth						keepSubPixelAA: keepSubPix].	^ _gsSqueakTemp_map