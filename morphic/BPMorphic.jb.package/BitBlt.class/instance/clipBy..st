accessing
clipBy: aRectangle 	| _gsSqueakTemp_aPoint _gsSqueakTemp_right _gsSqueakTemp_bottom |	_gsSqueakTemp_right := clipX + clipWidth.	_gsSqueakTemp_bottom := clipY + clipHeight.	_gsSqueakTemp_aPoint := aRectangle origin.	_gsSqueakTemp_aPoint x > clipX		ifTrue: [clipX := _gsSqueakTemp_aPoint x].	_gsSqueakTemp_aPoint y > clipY		ifTrue: [clipY := _gsSqueakTemp_aPoint y].	_gsSqueakTemp_aPoint := aRectangle corner.	_gsSqueakTemp_aPoint x < _gsSqueakTemp_right		ifTrue: [_gsSqueakTemp_right := _gsSqueakTemp_aPoint x].	_gsSqueakTemp_aPoint y < _gsSqueakTemp_bottom		ifTrue: [_gsSqueakTemp_bottom := _gsSqueakTemp_aPoint y].	clipWidth := _gsSqueakTemp_right - clipX.	clipHeight := _gsSqueakTemp_bottom - clipY.	clipWidth < 0		ifTrue: [clipWidth := 0].	clipHeight < 0		ifTrue: [clipHeight := 0]