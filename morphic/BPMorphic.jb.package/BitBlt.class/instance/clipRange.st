p_bbH + _gsSqueakTemp_sy.					_gsSqueakTemp_sy := 0].			_gsSqueakTemp_sy + _gsSqueakTemp_bbH > sourceForm height				ifTrue: [_gsSqueakTemp_bbH := _gsSqueakTemp_bbH - (_gsSqueakTemp_sy + _gsSqueakTemp_bbH - sourceForm height)]].	(_gsSqueakTemp_bbW <= 0			or: [_gsSqueakTemp_bbH <= 0])		ifTrue: [sourceX := sourceY := destX := destY := clipX := clipY := width := height := 0.			^ true].	(_gsSqueakTemp_sx = sourceX			and: [_gsSqueakTemp_sy = sourceY					and: [_gsSqueakTemp_dx = destX							and: [_gsSqueakTemp_dy = destY									and: [_gsSqueakTemp_bbW = width											and: [_gsSqueakTemp_bbH = height]]]]])		ifTrue: [^ false].	sourceX := _gsSqueakTemp_sx.	sourceY := _gsSqueakTemp_sy.	destX := _gsSqueakTemp_dx.	destY := _gsSqueakTemp_dy.	width := _gsSqueakTemp_bbW.	height := _gsSqueakTemp_bbH.	^ true