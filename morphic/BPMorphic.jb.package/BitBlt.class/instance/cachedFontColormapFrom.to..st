private
cachedFontColormapFrom: sourceDepth to: destDepth 	| _gsSqueakTemp_srcIndex _gsSqueakTemp_map |	CachedFontColorMaps class == Array		ifFalse: [CachedFontColorMaps := (1 to: 9)						collect: [:i | Array new: 32]].	_gsSqueakTemp_srcIndex := sourceDepth.	sourceDepth > 8		ifTrue: [_gsSqueakTemp_srcIndex := 9].	(_gsSqueakTemp_map := (CachedFontColorMaps at: _gsSqueakTemp_srcIndex)					at: destDepth) ~~ nil		ifTrue: [^ _gsSqueakTemp_map].	_gsSqueakTemp_map := (Color cachedColormapFrom: sourceDepth to: destDepth) copy.	(CachedFontColorMaps at: _gsSqueakTemp_srcIndex)		at: destDepth		put: _gsSqueakTemp_map.	^ _gsSqueakTemp_map