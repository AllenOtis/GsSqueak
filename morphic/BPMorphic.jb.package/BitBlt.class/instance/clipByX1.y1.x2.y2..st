accessing
clipByX1: x1 y1: y1 x2: x2 y2: y2 	| _gsSqueakTemp_right _gsSqueakTemp_bottom |	_gsSqueakTemp_right := clipX + clipWidth.	_gsSqueakTemp_bottom := clipY + clipHeight.	x1 > clipX		ifTrue: [clipX := x1].	y1 > clipY		ifTrue: [clipY := y1].	x2 < _gsSqueakTemp_right		ifTrue: [_gsSqueakTemp_right := x2].	y2 < _gsSqueakTemp_bottom		ifTrue: [_gsSqueakTemp_bottom := y2].	clipWidth := _gsSqueakTemp_right - clipX.	clipHeight := _gsSqueakTemp_bottom - clipY.	clipWidth < 0		ifTrue: [clipWidth := 0].	clipHeight < 0		ifTrue: [clipHeight := 0]