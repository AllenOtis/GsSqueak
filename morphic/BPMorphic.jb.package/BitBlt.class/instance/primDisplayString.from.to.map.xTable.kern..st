private
primDisplayString: aString from: startIndex to: stopIndex map: glyphMap xTable: xTable kern: kernDelta 	| _gsSqueakTemp_ascii |	self halt. "primitiveDisplayString"	startIndex		to: stopIndex		do: [:charIndex | 			_gsSqueakTemp_ascii := (aString at: charIndex) asciiValue.			glyphMap				ifNotNil: [_gsSqueakTemp_ascii := glyphMap at: _gsSqueakTemp_ascii + 1].			sourceX := xTable at: _gsSqueakTemp_ascii + 1.			width := (xTable at: _gsSqueakTemp_ascii + 2)						- sourceX.			self copyBits.			destX := destX + width + kernDelta]