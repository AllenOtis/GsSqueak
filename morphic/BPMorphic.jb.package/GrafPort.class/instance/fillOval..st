drawing support
fillOval: rect 	| _gsSqueakTemp_centerX _gsSqueakTemp_centerY _gsSqueakTemp_nextY _gsSqueakTemp_yBias _gsSqueakTemp_xBias _gsSqueakTemp_outer _gsSqueakTemp_nextOuterX |	rect area <= 0		ifTrue: [^ self].	height := 1.	_gsSqueakTemp_yBias := rect height odd				ifTrue: [0]				ifFalse: [-1].	_gsSqueakTemp_xBias := rect width odd				ifTrue: [1]				ifFalse: [0].	_gsSqueakTemp_centerX := rect center x.	_gsSqueakTemp_centerY := rect center y.	_gsSqueakTemp_outer := EllipseMidpointTracer new on: rect.	_gsSqueakTemp_nextY := rect height // 2.	[_gsSqueakTemp_nextY > 0]		whileTrue: [_gsSqueakTemp_nextOuterX := _gsSqueakTemp_outer stepInY.			width := (_gsSqueakTemp_nextOuterX bitShift: 1)						+ _gsSqueakTemp_xBias.			destX := _gsSqueakTemp_centerX - _gsSqueakTemp_nextOuterX.			destY := _gsSqueakTemp_centerY - _gsSqueakTemp_nextY.			self copyBits.			destY := _gsSqueakTemp_centerY + _gsSqueakTemp_nextY + _gsSqueakTemp_yBias.			self copyBits.			_gsSqueakTemp_nextY := _gsSqueakTemp_nextY - 1].	destY := _gsSqueakTemp_centerY.	height := 1 + _gsSqueakTemp_yBias.	width := rect width.	destX := rect left.	self copyBits